{% extends 'base.html.twig' %}

{% block title %}Hello AnimalController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
<li>Espece : {{animal.espece.libelle}} </li>
 <li>{{animal.nom}}  : {{animal.description}}</li>
 <li>Poid : {{animal.poid}}</li>
 <li>couleur : {{animal.couleur}}</li>
 <img src="{{animal.img}}" alt="photo">
 {% for dispose in animal.disposes %}
 <li>{{dispose.personne.nom}} en possede</li>
{% endfor %}
 


</div>



 </ul>
{% endblock %}
